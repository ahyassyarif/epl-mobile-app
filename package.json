		"start": "web-dev-server --port 8087 --node-resolve --base-path /petro_mon --app-index index.html",
		"dev": "webpack-dev-server --env.mode development --colors",
		"build": "webpack --config webpack.config.js",
		"build:stats": "webpack --profile --json >stats.json",
		"stats": "webpack-bundle-analyzer stats.json",
		"test": "npm run test:unit && npm run test:integration",
		"test:integration": "mocha test/integration --timeout=10000",
		"test:unit": "polymer test --module-resolution=node --npm",
